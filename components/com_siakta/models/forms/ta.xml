<?xml version="1.0" encoding="UTF-8"?>
<form>
    <fieldset name="frmTa">
        <field 
            name    = "title"
            type    = "textarea"
            label   = "COM_SIAKTA_FIELD_JUDUL_LABEL"
            required = "true"
			rows    = "5"
            class   = "input-xxlarge input-large-text"
			onchange = "let p=this.selectionStart;this.value=this.value.toUpperCase();this.setSelectionRange(p, p);"            
        />
    </fieldset>

	<fieldset name="akademik" label="COM_SIAKTA_HEADING_AKADEMIK_LABEL">
		
		<field 
			name		= "tahun"
			type		= "text"
			label		= "COM_SIAKTA_FIELD_TAHUN_LABEL"
			
			class		= "readonly"
			readonly	= "true"
		/>

		<field
			name 	= "mahasiswa"
			type	= "text"
			label	= "COM_SIAKTA_LABEL_MAHASISWA_NAME"
			readonly = "true"
			class	= "readonly"
		/>

		<field
			name 	= "prodi"
			type	= "text"
			label	= "COM_SIAKTA_LABEL_PRODI_NAME"
			readonly = "true"
			class	= "readonly"
		/>

		<field
			name 	= "konsentrasi"
			type	= "text"
			label	= "COM_SIAKTA_LABEL_KONSENTRASI_NAME"
			readonly = "true"
			class	= "readonly"
		/>
		
		
		<field 
			name		= "dosbing1"
			type		= "text"
			label	 	= "COM_SIAKTA_FIELD_DOSBING1_LABEL"

			readonly = "true"
			class	= "readonly"
		/>
		

		<field 
			name		= "dosbing2"
			type		= "text"
			label	 	= "COM_SIAKTA_FIELD_DOSBING2_LABEL"
			readonly = "true"
			class	= "readonly"
			/>
			
		
	</fieldset>

	<fieldset name="sidang">
		<field 
			name		= "sidang_proposal"
			type		= "text"
			label		= "COM_SIAKTA_FIELD_SIDANG_PROPOSAL_LABEL"
			readonly = "true"
		/>

		<field 
			name		= "sidang_akhir"
			type		= "text"
			label		= "COM_SIAKTA_FIELD_SIDANG_AKHIR_LABEL"
			readonly  	= "true"
		/>

		<field 
			name		= "ruang_sidang"
			type		= "text"
			label		= "COM_SIAKTA_FIELD_RUANG_SIDANG_LABEL"
			readonly  	= "true"
		/>
		<field 
			name		= "penguji1"
			type		= "text"
			label	 	= "COM_SIAKTA_FIELD_PENGUJI1_LABEL"
			readonly 	= "true"
		/>
			

		<field 
			name		= "penguji2"
			type		= "text"
			label	 	= "COM_SIAKTA_FIELD_PENGUJI2_LABEL"
			readonly 	= "true"
		/>
		

		<field 
			name		= "penguji3"
			type		= "text"
			label	 	= "COM_SIAKTA_FIELD_PENGUJI3_LABEL"
			readonly	= "true"
		/>
		<field 
			name		= "penguji4"
			type		= "text"
			label	 	= "COM_SIAKTA_FIELD_PENGUJI4_LABEL"
			readonly		= "true"
		/>
		

		<field 
			name		= "tanggal_lulus"
			type		= "text"
			label		= "COM_SIAKTA_FIELD_TANGGAL_LULUS_LABEL"
			readonly	= "true"
		/>

		<field 
			name		= "yudisium"
			type		= "text"
			label		= "COM_SIAKTA_FIELD_YUDISIUM_LABEL"
			readonly	= "true"
		 />
			
	</fieldset>

    <fieldset name="opsi">

		<field
			name="created_on"
			type="text"
			label="COM_SIAKTA_FIELD_CREATED_LABEL"
			size="22"
			translateformat="true"
			showtime="true"
			filter="user_utc"
			readonly = "true"
		/>

		<field
			name="created_by"
			type="user"
			label="JGLOBAL_FIELD_CREATED_BY_LABEL"
			description=""
			readonly = "true"
		/>


		<field
			name="last_update_on"
			type="calendar"
			label="JGLOBAL_FIELD_MODIFIED_LABEL"
			description=""
			class="readonly"
			size="22"
			readonly="true"
			translateformat="true"
			showtime="true"
			filter="user_utc"
		/>

		<field
			name="last_update_by"
			type="user"
			label="JGLOBAL_FIELD_MODIFIED_BY_LABEL"
			description=""
			class="readonly"
			readonly="true"
			filter="unset"
		/>

	</fieldset> 
</form>